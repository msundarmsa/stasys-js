{
  "name": "stasys-js",
  "productName": "stasys-js",
  "version": "1.0.0",
  "description": "STASYS",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start --enable-logging",
    "package": "electron-forge package && ./patch_scripts/worker_patch.sh",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx . --fix"
  },
  "keywords": [],
  "author": {
    "name": "Meenatchi Sundaram Muthu Selva Annamalai",
    "email": "mrmmsmsa.2k12@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_forge_template"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@types/d3": "^7.1.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "bindings": "^1.5.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "electron": "17.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "~4.5.4",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "d3": "^7.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "opencv-bindings": "file:opencv-bindings",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
